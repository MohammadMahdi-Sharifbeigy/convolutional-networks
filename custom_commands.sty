\def\persian@alph#1{%
  \ifcase#1\or آ\or ب\or ج\or د\or ه\or و\or ز\or ح\or ط\or ی\or ک\or ل\or م\or ن\or س\or ع\or ف\or ص\or ق\or ر\or ش\or ت\or ث\or خ\or ذ\or ض\or ظ\or غ\else\@ctrerr\fi
}
\makeatother

% Define Persian digits counter
\def\persian@num#1{%
  \ifcase#1\or ۱\or ۲\or ۳\or ۴\or ۵\or ۶\or ۷\or ۸\or ۹\or ۱۰\or ۱۱\or ۱۲\or ۱۳\or ۱۴\or ۱۵\or ۱۶\or ۱۷\or ۱۸\or ۱۹\or ۲۰\else\@ctrerr\fi
}

% Create new counter styles
\AddEnumerateCounter{\persianalph}{\persian@alph}{آ}
\AddEnumerateCounter{\persianum}{\persian@num}{۱}

% Define custom list environment for Persian nested enumeration
\newlist{persianlist}{enumerate}{2}
\setlist[persianlist,1]{
    label=\persianum*),
    leftmargin=1.25cm,
    font=\itshape
}
\setlist[persianlist,2]{
    label=\persianalph*),
    leftmargin=2cm,
    font=\itshape
}

% Alternative: Define a custom environment that combines both levels
\newenvironment{persianenum}{%
    \begin{enumerate}[
        label=\persianum*\persianalph*),
        leftmargin=1.25cm,
        font=\itshape,
        start=1
    ]
}{%
    \end{enumerate}
}

% Manual counter approach for mixed numbering (1آ, 1ب, 2آ, 2ب, etc.)
\newcounter{persiannumber}
\newcounter{persianletter}[persiannumber]
\renewcommand{\thepersianletter}{\persian@num{\value{persiannumber}}\persian@alph{\value{persianletter}}}

\newenvironment{mixedpersianenum}{%
    \setcounter{persiannumber}{1}
    \begin{list}{\itshape\thepersianletter)}{%
        \usecounter{persianletter}
        \setlength{\leftmargin}{1.25cm}
        \setlength{\itemsep}{0pt}
        \setlength{\parsep}{0pt}
    }
}{%
    \end{list}
}